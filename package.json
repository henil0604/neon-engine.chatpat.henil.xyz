{
  "name": "@chatpat/neon-engine",
  "version": "0.0.1",
  "description": "Backend engine for chatpat that is responsible for handing all kind of requests required",
  "main": ".neon-engine/index.js",
  "type": "module",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rfv .neon-engine node_modules",
    "tsx": "tsx ./app/index.ts",
    "tsx:watch": "tsx watch ./app/index.ts",
    "start": "node ./.neon-engine",
    "prepare": "prisma generate && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.15.3",
    "prisma": "^5.3.1",
    "tsup": "^6.6.3",
    "tsx": "^3.12.6",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@hapi/hapi": "^21.3.2",
    "@prisma/client": "^5.3.1",
    "dotenv": "^16.3.1",
    "zod": "^3.22.2"
  }
}